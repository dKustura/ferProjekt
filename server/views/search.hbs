{{> header }}

<main class="page">
  <h1> Search results </h1>

  {{#if users}}
    <ul>
      {{#each users}}
        <li>
          <a href="/profile/{{this.id}}">{{this.firstName}} {{this.lastName}}</a>
        </li>
      {{/each}}
    </ul>
  {{else}}
    <div> No results. </div>
  {{/if}}
</main>
